{
  "tabWidth": 2,
  "semi": true,
  "printWidth": 90,
  "singleQuote": false,
  "endOfLine": "lf",
  "trailingComma": "es5",
  "quoteProps": "preserve",
  "overrides": [
    {
      "files": ["**/*.jsonc", "**/tsconfig*.json"],
      "options": {
        "trailingComma": "none"
      }
    },
    {
      "files": ["pnpm-workspace.yaml"],
      "options": {
        "singleQuote": true
      }
    }
  ],
  "plugins": ["prettier-plugin-organize-imports"]
}
